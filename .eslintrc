{
	"globals": {
		"React": false,
		"ReactDOM": false,
		"Set": false
	},
	"env": {
		// I write for browser
		"browser": true,
		// in CommonJS
		"node": true
	},
	// "parser": "babel-eslint",
	// To give you an idea how to override rule options:
	"rules": {
		// Tons of rules you can use, for example...
		"quotes": [1, "single"],
		"strict": 1,
		"no-undef": 1
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"impliedStrict": true
		},
	},
}